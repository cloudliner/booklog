<!--参考: https://github.com/k2works/sinatra_rakuten_api-->
<div class="starter-template">
  <h1>Books written by...</h1>
    <table class="table table-hover">
      <tr>
        <th>書籍名</th>
        <th>出版日</th>
      </tr>
      <% @books.each do |book| %>            
      <tr>            
        <td><%= book.title %></td>
        <td><%= book['salesDate'] %></td>
      </tr>
      <% end %>                    
    </table>          
</div>
